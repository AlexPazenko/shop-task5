<form method="GET" class="form-inline my-0 mr-md-auto" action="{{ path('order_search_results') }}" style="align-items: end;">
    <div>
    <label for="form_fromDate" class="required">Search orders From:</label>
    <input type="date" id="form_fromDate" name="fromDate" required="required" placeholder="yyyy-mm-dd" class="form-control" />
    </div>
    <div>
    <label for="form_toDate" class="required">Search orders To:</label>
    <input type="date" id="form_toDate" name="toDate" required="required" placeholder="yyyy-mm-dd" class="form-control ml-3" />
    </div>
    <button class="btn btn-primary ml-3" type="submit">Search orders</button>
</form>